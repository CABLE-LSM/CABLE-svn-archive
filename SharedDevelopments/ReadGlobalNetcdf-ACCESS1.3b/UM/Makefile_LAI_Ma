FC=ifort
CFLAGS=-O2 
LSRC = cable_ncdf.F90

#suffixes we use
.SUFFIXES:
.SUFFIXES: .F90 .o 
#default rules for these suffixes
.F90.o:
	$(FC) $(CFLAGS) $(CINC) -c $<

## default target by convention is ``all''
all : cable_ncdf.o 
