*msksum=landsea.sum();
*npts=$lat.size*$lon.size;
if (msksum > npts/2){
   print("Assuming mask has land=0,sea=1 because sum(mask) > 1/2 total points\n");
   where(lat2D < latmin || lat2D > latmax || lon2D < lonmin || lon2D > lonmax) landsea=1;
}
else{
   print("Assuming mask has land=1,sea=0 because sum(mask) <= 1/2 total points\n");
   where(lat2D < latmin || lat2D > latmax || lon2D < lonmin || lon2D > lonmax) landsea=0;

}
