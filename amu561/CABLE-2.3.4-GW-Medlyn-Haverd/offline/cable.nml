   gswpfile%snowf = "./gswp/Snowf/GSWP3.BC.Snowf.3hrMap.0.nc"
   gswpfile%LWdown= "./gswp/LWdown/GSWP3.BC.LWdown.3hrMap.0.nc"
   gswpfile%SWdown= "./gswp/SWdown/GSWP3.BC.SWdown.3hrMap.0.nc" 
