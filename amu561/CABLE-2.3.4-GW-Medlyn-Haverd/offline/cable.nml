   cable_user%GW_MODEL = .FALSE.       !True means use the groundwater module, false means use default soil snow scheme
   cable_user%alt_forcing = .false.
   cable_user%GSWP3 = .FALSE.
   cable_user%GS_SWITCH = 'medlyn'      !choices are:
                                        ! 1. leuning
                                        ! 2. medlyn

   cable_user%L_newProfile = .FALSE.   !Use new YP profile for root water extraction?
   cable_user%or_evap = .FALSE.        !Use alternative Or SoilE formulation?

&end

